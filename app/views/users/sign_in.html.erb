<%= form_for :user, url: try_enter_users_path do |f| %>
    <div class="field">
        <%= f.label :name %><br>
        <%= f.text_field :name, :autofocus => true %>
    </div>
    <div class="field">
        <%= f.label :password %><br>
        <%= f.password_field :password %>
    </div>
    <br>
    <div class="actions">
        <%= f.submit 'Login' %>
    </div>
<% end %>